datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Company {
  id      Int      @id @default(autoincrement())
  name    String
  persons Person[]
}

model Person {
  id        Int      @id @default(autoincrement())
  name      String
  company   Company? @relation(fields: [companyId], references: [id])
  companyId Int?
}
